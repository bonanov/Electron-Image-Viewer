{
  "build": {
    "asar": true,
    "appId": "Banana",
    "mac": {
      "category": "your.app.category.utilities"
    },
    "linux": {
      "category": "Graphics"
    },
    "win": {
      "files": [
        "build/**/*",
        "build/*",
        "!build/static/js/Components",
        "!build/static/js/constants",
        "!build/static/js/reducers",
        "!build/static/js/utils",
        "!build/static/js/App.js",
        "!build/static/js/index.js",
        "!build/static/js/serviceWorker.js",
        "node_modules/**/*"
      ]
    },
    "files": [
      "build/**/*",
      "build/*",
      "!build/static/js/Components",
      "!build/static/js/constants",
      "!build/static/js/reducers",
      "!build/static/js/utils",
      "!build/static/js/App.js",
      "!build/static/js/index.js",
      "!build/static/js/serviceWorker.js",
      "node_modules/**/*"
    ],
    "asarUnpack": [
      "node_modules/gm",
      "node_modules/sharp"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "name": "image-viewer",
  "email": "a39345@outlook.com",
  "description": "image viewer aye",
  "author": "Bonan <john@doe.com>",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "maineed": "src/electron-starter.prod.js",
  "dependencies": {
    "config": "^3.1.0",
    "electron-store": "^3.2.0",
    "fast-average-color": "^5.1.0",
    "font-awesome": "^4.7.0",
    "gm": "^1.23.1",
    "hash.js": "^1.1.7",
    "lodash": "^4.17.11",
    "node": "^12.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "sharp": "^0.22.1",
    "stackblur": "^1.0.0",
    "stackblur-canvas": "^2.2.0",
    "trash": "^5.2.0"
  },
  "scripts": {
    "all": "run-p watch electron-dev",
    "watch": "run-p start watch-less",
    "watch-less": "less-watch-compiler src/less/ src/css-auto/ main.less",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-app": "npm-run-all build bundle-dev bundle-move electron-build",
    "bundle-dev": "webpack src/secondWindow/utilApp.js -o src/secondWindow/bundle.js",
    "bundle-move": "cp src/secondWindow/ build/static/js/secondWindow/ -r",
    "electron-rebuild": "electron-rebuild",
    "flow": "flow",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "deb64": "electron-installer-debian --src image-viewer-linux-x64/ --dest image-viewer-linux-x64/installers/ --arch amd64",
    "electron-build": "electron-packager . --overwrite --ignore=/node_modules/.bin --ignore=/public",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron ./src/electron-starter.js",
    "pack": "electron-builder --dir -l",
    "dist": "electron-builder -l pacman",
    "dist-win": "electron-builder -w portable",
    "postinstall": "electron-builder install-app-deps",
    "electron-pack": "build -l"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "electron": "^5.0.1",
    "electron-builder": "^20.41.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-debian": "^1.2.0",
    "electron-rebuild": "^1.8.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.17.1",
    "react-app-rewired": "^2.1.3",
    "typescript": "^3.4.5",
    "webpack-cli": "^3.3.2",
    "worker-loader": "^2.0.0",
    "worker-plugin": "^3.1.0"
  }
}
