{
  "name": "image-viewer",
  "description": "image viewer aye",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron-starter.prod.js",
  "dependencies": {
    "config": "^3.1.0",
    "electron": "^5.0.1",
    "font-awesome": "^4.7.0",
    "gm": "^1.23.1",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "sharp": "^0.22.1"
  },
  "scripts": {
    "all": "run-p watch electron-dev",
    "watch": "run-p start watch-less",
    "watch-less": "less-watch-compiler src/less/ src/css-auto/ main.less",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-app": "npm-run-all build electron-build",
    "flow": "flow",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "deb64": "electron-installer-debian --src image-viewer-linux-x64/ --dest image-viewer-linux-x64/installers/ --arch amd64",
    "electron-build": "electron-packager . --overwrite --ignore=/node_modules/.bin --ignore=/public",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron ./src/electron-starter.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-debian": "^1.2.0",
    "electron-rebuild": "^1.8.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.17.1",
    "react-app-rewired": "^2.1.3",
    "worker-loader": "^2.0.0",
    "worker-plugin": "^3.1.0"
  }
}
